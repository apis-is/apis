          'D'   : { 'is': 'Vindstefna',
                    'en': 'Wind direction'},
          'T'   : { 'is': 'Hiti (°C)',
                    'en': 'Air temperature (°C)'},
          'W'   : { 'is': 'Veðurlýsing',
                    'en': 'Weather description'},
          'V'   : { 'is': 'Skyggni (km)',
                    'en': 'Visibility (km)'},
          'N'   : { 'is': 'Skýjahula (%)',
