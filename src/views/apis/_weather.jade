extends ../shared/_endpoint

block endpointvars
  - var shortname = 'weather'
  - var title = 'Icelandic Weather'
  - var icon = 'sun-o'

block sources
  h4
    | Source:&nbsp;
    a(href='http://vedur.is' target="_blank") Icelandic Meteorological Office

block endpoints
  box.light.endpoint.disabled
    +disabledtitlebar('get','/weather/:source/:lang')
    .content
      p
        | Get weather information for Iceland.
        br
        | Under 'Descriptions' you will find a list that will help you understand the output of the following endpoints.

      h5.params(toggle-next) Parameters
      dl.params
        dt.req /:type
        dd Type of information to get (forecasts|observations|texts)
        dt /:lang
        dd Language of output ('en' or 'is'), defaults to 'is'

      h5.descriptions(toggle-next='closed') Descriptions
      pre.descriptions: code.javascript.
        var descriptions = {
          'F'   : { 'is': 'Vindhraði (m/s)',
                    'en': 'Wind speed (m/s)'},
          'FX'  : { 'is': 'Mesti vindhraði (m/s)',
                    'en': 'Top wind speed (m/s)'},
          'FG'  : { 'is': 'Mesta vindhviða (m/s)',
